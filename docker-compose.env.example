# Przykładowy plik .env dla docker-compose.dev.yml
# Skopiuj ten plik jako .env w głównym katalogu projektu i wypełnij wartościami
# NIE COMMITUJ pliku .env do repozytorium!

# Connection String do SQL Server
SQL_CONNECTION_STRING=Server=host.docker.internal,1433;Database=AITS-React;User Id=aitsadmin;Password=YOUR_PASSWORD;MultipleActiveResultSets=true;TrustServerCertificate=True

# JWT Secret Key (min. 32 znaki)
JWT_KEY=YOUR_JWT_SECRET_KEY_MIN_32_CHARS_LONG

# Tpay Configuration
Tpay__ApiUrl=https://openapi.sandbox.tpay.com
Tpay__ClientId=YOUR_TPAY_CLIENT_ID
Tpay__ClientSecret=YOUR_TPAY_CLIENT_SECRET
Tpay__NotificationUrl=https://your-domain.com/api/payments/notify


