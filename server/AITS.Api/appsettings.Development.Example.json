{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AITS.Api.Services.AzureSpeechService": "Debug",
      "AITS.Api.Services.Realtime": "Debug",
      "AITS.Api.Hubs.TranscriptionHub": "Debug"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=localhost;Database=AITS-React;Trusted_Connection=true;Encrypt=False;MultipleActiveResultSets=true;TrustServerCertificate=True"
  },
  "Email": {
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 587,
    "Username": "your-dev-email@gmail.com",
    "Password": "your-app-password",
    "FromEmail": "noreply@aits.local",
    "FromName": "AI Therapy Support Dev",
    "UseSsl": true
  },
  "GoogleOAuth": {
    "ClientId": "YOUR_GOOGLE_CLIENT_ID",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "DefaultRedirectUri": "http://localhost:7100/api/integrations/google-calendar/callback"
  },
  "GoogleCalendar": {
    "CalendarId": "YOUR_GOOGLE_CALENDAR_ID"
  },
  "AzureAI": {
    "Endpoint": "https://your-resource.openai.azure.com/",
    "ApiKey": "YOUR_AZURE_AI_API_KEY",
    "ModelName": "gpt-4.1",
    "ModelVersion": "2025-01-01-preview",
    "MaxTokens": 1500,
    "Temperature": 0.7
  },
  "AzureSpeech": {
    "SubscriptionKey": "YOUR_AZURE_SPEECH_KEY",
    "Region": "swedencentral",
    "Language": "pl-PL",
    "MaxSpeakerCount": 3
  },
  "Tpay": {
    "ApiUrl": "https://openapi.sandbox.tpay.com",
    "ClientId": "YOUR_TPAY_CLIENT_ID",
    "ClientSecret": "YOUR_TPAY_CLIENT_SECRET",
    "NotificationUrl": "http://localhost:7100/api/payments/notify",
    "ReturnUrl": "http://localhost:7101/payment/return",
    "MerchantPosUrl": "http://localhost:7101/payment/merchant"
  }
}


